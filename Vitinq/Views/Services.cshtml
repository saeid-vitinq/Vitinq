@inherits Umbraco.Web.Mvc.UmbracoTemplatePage<ContentModels.Services>
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
    Layout = "Layout.cshtml";
    Services services = Model.Content;
    ServicesDT servicesDT = services.Main.Current as ServicesDT;
    List<Service> serviceList = services.Children<Service>( ).ToList( );
}
<div class="site-blocks-cover inner-page-cover overlay" style="background-image: url(@servicesDT.Banner.Url);" data-aos="fade" data-stellar-background-ratio="0.5">
    <div class="container">
        <div class="row align-items-center justify-content-center text-center">
            <div class="col-md-12" data-aos="fade-up" data-aos-delay="400">

                <div class="row justify-content-center mb-4">
                    <div class="col-md-8 text-center">
                        <h1>@servicesDT.Title</h1>
                        <p class="lead mb-5">@servicesDT.SubTitle</p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<section class="site-section border-bottom">
    <div class="container">
        <div class="row align-items-stretch">
            @foreach( Service item in serviceList )
            {
                ServiceDT serviceDT = item.Main.Current as ServiceDT;
                <div class="col-md-6 col-lg-4 mb-4 mb-lg-4" data-aos="fade-up">
                    <div class="unit-4 d-flex">
                        @*icon-laptop2*@
                        <div class="unit-4-icon mr-4"><span class="text-primary "><img src="@serviceDT.Icon.Url" alt="@serviceDT.Icon.Name" /></span></div>
                        <div>
                            <h3>@serviceDT.Title</h3>
                            <p>@serviceDT.Description</p>
                            @*<p><a href="#">Learn More</a></p>*@
                        </div>
                    </div>
                </div>
            }
